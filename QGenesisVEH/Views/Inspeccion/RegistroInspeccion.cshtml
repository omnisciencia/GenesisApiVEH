
@{
    ViewBag.Title = "RegistroInspeccion";
}

<link href="~/Content/styleRegInspeccion.css" rel="stylesheet" />

<div class="top-content">
    <div class="container">

        <div class="row">
            <div class="col-sm-12 col-sm-offset-1 col-md-11 col-md-offset-2 col-lg-11 col-lg-offset-3 form-box">
                <form role="form" action="" method="post" class="f1">

                    <h3 id="titulo">Registro de Inspección</h3>

                    <div class="f1-steps">
                        <div class="f1-progress">
                            <div class="f1-progress-line" data-now-value="16.66" data-number-of-steps="3" style="width: 16.66%;"></div>
                        </div>
                        <div class="f1-step active">
                            <div class="f1-step-icon"><i class="fas fa-user"></i></div>
                            <p>Contratante</p>
                        </div>
                        <div class="f1-step">
                            <div class="f1-step-icon"><i class="fas fa-user-secret"></i></div>
                            <p>Poliza e Inspección</p>
                        </div>
                        <div class="f1-step">
                            <div class="f1-step-icon"><i class="fas fa-car-side"></i></div>
                            <p>Vehículo</p>
                        </div>
                        <div class="f1-step">
                            <div class="f1-step-icon"><i class="fab fa-reddit-alien"></i></div>
                            <p>Accesorios</p>
                        </div>
                        <div class="f1-step">
                            <div class="f1-step-icon"><i class="fas fa-file-contract"></i></div>
                            <p>Datos Adicionales</p>
                        </div>
                    </div>
                    <br>
                    <fieldset>
                        <h4>Datos del Contratante:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Tipo Contratante:</label>
                                    <input type="text" class="form-control input-sm" id="tipocontra_id" onKeyUp="this.value=this.value.toUpperCase();">                                    
                                </div>
                            </div>
                        </div>
                        <div class="row">

                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Tipo Doc.Identidad:</label>
                                    <input type="text" class="form-control input-sm" id="tipodoc_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Nro. Documento:</label>
                                    <input type="text" class="form-control input-sm" id="dni_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-xs-12">
                                <div class="form-group">
                                    <label>Nombres:</label>
                                    <input type="text" class="form-control input-sm" id="nombre_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">

                                <div class="form-group">
                                    <label>Nacionalidad:</label>
                                    <select id="sp_Nacionalidad" class="form-control input-sm">
                                        <option></option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Teléfono:</label>
                                    <input type="text" class="form-control input-sm" id="fono_id" onkeypress="return justNumbers(event);">
                                </div>



                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">

                                <div class="form-group">
                                    <label>Estado Civil:</label>
                                    <select class="form-control input-sm" id="sp_EstadoCivil">
                                        <option></option>
                                    </select>
                                </div>

                                <div class="form-group">
                                    <label>Celular:</label>
                                    <input type="text" class="form-control input-sm" id="cel_id" onkeypress="return justNumbers(event);">
                                </div>


                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">


                                <div class="form-group">
                                    <label>Fecha Nacimiento:</label>
                                    <input type="date" class="form-control input-sm" id="fecnaci_id">
                                </div>
                                <div class="form-group">
                                    <label>Sexo:</label>
                                    <select id="sp_Sexo" class="form-control input-sm">
                                        <option></option>
                                    </select>
                                </div>


                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">


                                <div class="form-group">
                                    <label>E-mail:</label>
                                    <input type="text" class="form-control input-sm" id="email_id">
                                </div>

                            </div>
                        </div>

                        <div class="f1-buttons">
                            <button type="button" class="btn btn-next">Siguiente</button>
                        </div>

                    </fieldset>

                    <fieldset>
                        <h4>Datos de la Poliza:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Nro. Poliza:</label>
                                    <input type="text" class="form-control input-sm" id="nropoliza_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                                <div class="form-group">
                                    <label>Forma de Pago:</label>
                                    <select class="form-control input-sm" id="sp_FormaPago">
                                        <option></option>
                                    </select>
                                </div>

                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Plan/Producto:</label>
                                    <input type="text" class="form-control input-sm" id="plan_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Vigencia:</label>
                                    <input type="text" class="form-control input-sm" id="vigencia_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Tipo Poliza:</label>
                                    <input type="text" class="form-control input-sm" id="tipoliza_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                            </div>
                        </div>

                        <h4>Datos de la Inspección:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-8 col-md-8 col-xs-12">
                                <div class="form-group" id="chkbox_Prog">
                                    <input type="checkbox" name="chk_fijar_fecha" id="chk_fijar_fecha"><label class="col-form-label col-form-label-sm">&nbsp;&nbsp;<span id="text_habProg">Establecer Fecha de Programación</span></label>
                                </div>
                                <div class="form-group" id="chkbox_Reprog">
                                    <input type="checkbox" name="chk_fijar_Reprog" id="chk_fijar_Reprog"><label class="col-form-label col-form-label-sm">&nbsp;&nbsp;Establecer Fecha de Reprogramación</label>
                                </div>
                            </div>
                        </div>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Fecha de Programación:</label>
                                    <input type="date" class="form-control input-sm" id="fecinspeccion_id">
                                </div>
                                <div class="form-group">
                                    <label>Fecha de Recordatorio:</label>
                                    <input type="date" class="form-control input-sm" id="fecrecordatorio_id">
                                </div>

                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Hora de Programación:</label>
                                    <input type="time" class="form-control input-sm" id="hrinspeccion_id">
                                </div>
                                <div class="form-group">
                                    <label>Hora de Recordatorio:</label>
                                    <input type="time" class="form-control input-sm" id="hrecordatorio">
                                </div>

                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Estado:</label>
                                    <input type="text" class="form-control input-sm" id="estadoinspecc_id" onKeyUp="this.value=this.value.toUpperCase();">
                                    @*<label>Estado:</label>
                                    <select class="form-control input-sm" id="estadoinspecc_id" name="estadoinspecc_id">
                                        <option value="">PENDIENTE</option>
                                    </select>*@
                                </div>

                            </div>


                        </div>

                        <div class="f1-buttons">
                            <button id="btn_GuardarDI" type="button" class="btn btn-success" onclick="GuardarDI()">Guardar</button>
                            <button type="button" class="btn btn-previous">Anterior</button>
                            <button id="btn_SiguienteDI" type="button" class="btn btn-next">Siguiente</button>
                        </div>
                    </fieldset>
                    <fieldset>
                        <h4>Datos del Vehículo:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Clase Vehículo:</label>
                                    <select class="form-control input-sm" id="sp_TipoVehiculo">
                                        <option></option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Color:</label>
                                    <input type="text" class="form-control input-sm" id="colorveh_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                                <div class="form-group">
                                    <label>Nro. Asientos:</label>
                                    <input type="text" class="form-control input-sm" id="puertasveh_id" onKeyUp="this.value=this.value.toUpperCase();"  onkeypress="return justNumbers(event);">
                                </div>
                                @*<div class="form-group">
                                    <label>Nro. Serie:</label>
                                    <input type="text" class="form-control input-sm" id="nroserieveh_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>*@


                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Marca:</label>
                                    <select id="sp_MarcaVehiculo" class="form-control input-sm">
                                        <option></option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Transmisión:</label>
                                    <select id="sp_TipoTransmision" class="form-control input-sm"></select>
                                </div>
                                <div class="form-group">
                                    <label>Tipo Carrocería:</label>
                                    <select id="sp_TipoCarroceria" class="form-control input-sm"></select>
                                </div>
                                <div class="form-group">
                                    <label>Kilometraje:</label>
                                    <input type="text" class="form-control input-sm" id="kilometrajeveh_id">
                                </div>

                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Modelo:</label>
                                    <select id="sp_ModeloVehiculo" class="form-control input-sm">
                                        <option></option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>Forma Rodante:</label>
                                    <select id="sp_ClaseRodante" class="form-control input-sm"></select>
                                </div>
                                <div class="form-group">
                                    <label>Nro. Motor:</label>
                                    <input type="text" class="form-control input-sm" id="motorveh_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>
                                <div class="form-group">
                                    <label>VIN:</label>
                                    <input type="text" class="form-control input-sm" id="vinveh_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>


                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Año:</label>
                                    <input type="text" class="form-control input-sm" id="anioveh_id"  onkeypress="return justNumbers(event);">
                                </div>
                                <div class="form-group">
                                    <label>Tipo Combustible:</label>
                                    <select id="sp_TipoCombustible" class="form-control input-sm"></select>
                                </div>
                                <div class="form-group">
                                    <label>Placa:</label>
                                    <input type="text" class="form-control input-sm" id="placaveh_id" onKeyUp="this.value=this.value.toUpperCase();">
                                </div>




                            </div>


                        </div>

                        <div class="f1-buttons">
                            <button type="button" class="btn btn-previous">Anterior</button>
                            <button type="button" class="btn btn-next">Siguiente</button>                            
                        </div>
                    </fieldset>

                    <fieldset>
                        <h4>Accesorios del Vehículo:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_aireacond_id">
                                    <label class="form-check-label">Aire Acondicionado</label>
                                </div>
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_lunelectrica_id">
                                    <label class="form-check-label">Lunas Eléctricas</label>
                                </div>



                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">

                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_alarma_id">
                                    <label class="form-check-label">Alarma</label>
                                </div>
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_segruedas_id">
                                    <label class="form-check-label">Seguro de Ruedas</label>
                                </div>

                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_pestilloe_id">
                                    <label class="form-check-label">Pestillos Eléctricos</label>
                                </div>
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_llantarep_id">
                                    <label class="form-check-label">Llanta de Repuesto</label>
                                </div>


                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_tapizc_id">
                                    <label class="form-check-label">Tapiz de Cuero</label>
                                </div>



                            </div>


                        </div>



                        <h4>Accesorios Musicales:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_radoriginal_id">
                                    <label class="form-check-label">Radio Original</label>
                                </div>

                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_parlanteo_id">
                                    <label class="form-check-label">Parlantes Originales</label>
                                </div>

                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group form-check">
                                    <input class="form-check-input" type="checkbox" style="margin-top: 0.5rem;" id="chk_accesomusi_id">
                                    <label class="form-check-label">Otros Accesorios Musicales</label>
                                </div>

                            </div>
                            <div class="col-lg-3 col-md-3 col-xs-12">

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-xs-12">
                                <div class="form-group">
                                    <textarea row="3" class="form-control input-sm" placeholder="Observaciones..." id="accobservaciones_id" onKeyUp="this.value=this.value.toUpperCase();"></textarea>
                                </div>

                            </div>
                        </div>

                        <h4>Daños del Vehículo:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">
                                    <label>Tipo de Daño:</label>
                                    <select class="form-control input-sm" id="sp_TipoDano">
                                        <option>[Seleccione]</option>
                                    </select>
                                </div>
                            </div>

                        </div>


                        <h4>Observaciones:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-xs-12">
                                <div class="form-group">
                                    <textarea row="3" class="form-control input-sm" id="observaciones_id" onKeyUp="this.value=this.value.toUpperCase();"></textarea>
                                </div>

                            </div>
                        </div>



                        <div class="f1-buttons">
                            <button type="button" class="btn btn-previous">Anterior</button>
                            <button type="button" class="btn btn-next">Siguiente</button>
                        </div>
                    </fieldset>

                    <fieldset>
                        <h4>Seleccionar Accesorios:</h4>
                        <div style="height: 10px;"></div>
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-xs-12">
                                <div class="form-group">

                                    <label class="form-check-label">Faros Delanteros</label>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <select class="form-control input-sm" id="sp_farDelantero">
