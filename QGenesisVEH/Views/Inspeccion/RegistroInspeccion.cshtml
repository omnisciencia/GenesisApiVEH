                                                <option value="2">Roto</option>
                                                <option value="3">Mal estado</option>
                                                <option value="4">No Aplica</option>
                                            </select>
                                        </div>
                                        <div class="col-md-6">
                                            <input id="cant_FarDelantero" value="0" type="number" class="form-control form-control-sm" placeholder="Cantidad" min="0" pattern="^[0-9]+" />
                                        </div>
                                    </div>


                                </div>
                                <div class="form-group">
                                    <label class="form-check-label">Faros Posteriores</label>
